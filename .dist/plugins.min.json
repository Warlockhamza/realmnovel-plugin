{
  "name": "RealmNovel",
  "pluginId": "realmnovel",
  "version": "1.0.0",
  "language": "en",
  "type": "novel",
  "url": "https://www.realmnovel.com",
  "nsfw": false,
  "entry": {
    "novel": "/novel/{{slug}}",
    "chapter": "/novel/{{slug}}/{{chapterSlug}}",
    "search": "/search?q={{query}}"
  },
  "selectors": {
    "novelTitle": "h1",
    "novelCover": ".book-cover img",
    "novelSummary": ".book-desc",
    "chapterList": ".chapter-list a",
    "chapterTitle": ".chapter-title",
    "chapterContent": ".chapter-content"
  }
}
