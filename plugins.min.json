{
  "name": "RealmNovel",
  "pluginId": "realmnovel",
  "version": "1.0.0",
  "language": "ar",
  "type": "novel",
  "url": "https://www.realmnovel.com",
  "nsfw": false,
  "entry": {
    "novelList": "/library",
    "search": "/search?q={{query}}",
    "novel": "/novel/{{slug}}",
    "chapter": "/novel/{{slug}}/{{chapterSlug}}"
  },
  "selectors": {
    "novelItem": ".book-item",
    "novelTitle": ".book-title, h1",
    "novelCover": "img",
    "novelLink": "a",
    "novelSummary": ".book-desc, .description, .summary",
    "chapterList": ".chapter-list a",
    "chapterTitle": ".chapter-title, h1",
    "chapterContent": ".chapter-content, .content"
  }
}
